{
  "name": "cms-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "mensajes",
    "mensajesback",
    "backend"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=mensajes & npm run dev --workspace=mensajesback & npm run start --workspace=backend",
    "dev:frontend": "npm run dev --workspace=mensajes",
    "dev:admin": "npm run dev --workspace=mensajesback",
    "start:backend": "npm run start --workspace=backend"
  }
}
